{
  "name": "notion-proxy-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker proxy and orchestration layer for the Notion SDK",
  "main": "src/index.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "wrangler dev",
    "deploy": "npm run build && npm run migrate:remote && wrangler deploy",
    "migrate:remote": "wrangler d1 migrations apply DB --remote",
    "typecheck": "tsc --noEmit",
    "build": "tsc && npm --prefix frontend run build",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "notion",
    "proxy",
    "api"
  ],
  "author": "",
  "dependencies": {
    "@notionhq/client": "^5.3.0",
    "hono": "^4.0.0",
    "zod": "^3.22.4",
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "openapi3-ts": "^4.2.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240725.0",
    "@types/node": "^24.10.0",
    "prettier": "^2.7.1",
    "typescript": "^5.3.3",
    "wrangler": "^4.45.4"
  }
}
