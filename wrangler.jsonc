{
  "name": "core-notion-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-11-02",
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "NODE_ENV": "production"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "notion-proxy",
      "database_id": "1cfda39d-e176-432b-a89e-a751cc555d5b",
      "migrations_dir": "migrations"
    }
  ],
  "ai": {
    "binding": "AI"
  }
}
